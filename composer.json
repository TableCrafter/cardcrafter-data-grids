{
    "name": "tablecrafter/cardcrafter-data-grids",
    "description": "Data-Driven Card Grids for WordPress",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "mockery/mockery": "^1.6",
        "10up/wp_mock": "0.4.2"
    },
    "autoload": {
        "psr-4": {
            "CardCrafter\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
